{
  "name": "diffhtml",
  "version": "0.3.0",
  "description": "",
  "main": "dist/diffhtml",
  "author": "Tim Branyen (@tbranyen)",
  "repository": "https://github.com/tbranyen/diffhtml",
  "license": "MIT",
  "scripts": {
    "start": "http-server",
    "build": "browserify -t babelify -s diff lib/index.js | derequire > dist/diffhtml.js",
    "build-test": "browserify -t babelify -s diff lib/index.js -o dist/diffhtml.js -g requireify -t requireify",
    "watch": "watchify -t babelify -t requireify -g requireify -d -s diff lib/index.js -o dist/diffhtml.js -v",
    "jshint": "jshint lib/*.js",
    "mocha": "mocha-phantomjs test/index.html",
    "test": "npm run jshint && npm run build-test && npm run mocha"
  },
  "devDependencies": {
    "babel-core": "^5.2.17",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "browserify-istanbul": "^0.2.1",
    "custom-event": "^1.0.0",
    "customevent-shim": "^0.1.0",
    "del": "^2.2.0",
    "derequire": "^2.0.2",
    "glob": "^6.0.2",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.0.0",
    "gulp-jshint": "^2.0.0",
    "gulp-load-plugins": "^1.1.0",
    "gulp-mocha": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-util": "^3.0.7",
    "http-server": "^0.8.5",
    "isparta": "^4.0.0",
    "istanbul": "^0.3.22",
    "jshint": "^2.8.0",
    "karma": "^0.13.15",
    "karma-coverage": "douglasduteil/karma-coverage#next",
    "mocha": "^2.3.3",
    "mocha-phantomjs": "^4.0.1",
    "mocha-phantomjs-istanbul": "^0.0.2",
    "path": "^0.12.7",
    "promise-polyfill": "^2.1.0",
    "requireify": "^1.0.0",
    "simple-jsdom": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.4.0",
    "weakmap": "^0.0.6",
    "webpack": "^1.12.9",
    "webpack-stream": "^3.1.0"
  }
}
